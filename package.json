{
  "name": "electronjssample",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager . electronjssample --platform=linux --arch=x64 --out=release --overwrite",
    "package-windows": "electron-packager . electronjssample --platform=win32 --arch=x64 --out=release --overwrite",
    "package-macos": "electron-packager . electronjssample --platform=darwin --arch=x64 --out=release --overwrite"
  },
  "license": "ISC",
  "dependencies": {
    "electron": "^33.2.0",
    "electron-packager": "^17.1.2"
  }
}
